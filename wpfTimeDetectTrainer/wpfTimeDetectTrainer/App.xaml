<Application x:Class="wpfTimeDetectTrainer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wpfTimeDetectTrainer"
             xmlns:converters="clr-namespace:TimeTrainer.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converters:MultiplyConverter x:Key="MultiplyConverter"/>
            <converters:EnumToBooleanConverter x:Key="EnumToBooleanConverter"/>
            <converters:StringStartsWithConverter x:Key="StringStartsWithConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>